{
  "name": "range-sdk",
  "description": "Range SDK for node",
  "main": "lib/range.js",
  "type": "module",
  "version": "1.6.0",
  "scripts": {
    "lint": "./node_modules/.bin/eslint **/*.js",
    "fmt": "./node_modules/.bin/prettier --write **/*.js"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "prettier": "^1.17.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent"
  },
  "directories": {
    "lib": "lib"
  },
  "main": "lib/range.js",
  "type": "module"
}
